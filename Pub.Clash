port: 7890
socks-port: 7891
allow-lan: true
mode: Rule
log-level: info
external-controller: 0.0.0.0:9090
proxy-groups:
  - name: 🗿 节点选择
    type: select
    proxies:
      - ♻️ 自动选择
      - DIRECT
    use:
      - BiHaiNode
      - NullNode
      - OpenitNode
  - name: ♻️ 自动选择
    type: url-test
    url: http://www.gstatic.com/generate_204
    interval: 300
    tolerance: 50
    proxies:
    use:
      - BiHaiNode
      - NullNode
      - OpenitNode
  - name: 🚫 运营劫持
    type: select
    proxies:
      - 🛑 全球拦截
      - 🎯 全球直连
      - 🗿 节点选择
  - name: 🎯 全球直连
    type: select
    proxies:
      - DIRECT
      - 🗿 节点选择
      - ♻️ 自动选择
  - name: 🛑 全球拦截
    type: select
    proxies:
      - REJECT
      - DIRECT
  - name: 🐟 漏网之鱼
    type: select
    proxies:
      - 🗿 节点选择
      - DIRECT
    use:
      - BiHaiNode
      - NullNode
      - OpenitNode
proxy-providers:
  BiHaiNode:
    type: http
    url: "https://proxies.bihai.cf/clash/proxies?c=CN,HK,TW,RU,US,JP,KR,SG"
    path: BiHai.Clash/BiHaiNode.yaml
    interval: 3600
    health-check:
      enable: true
      interval: 600
      url: http://www.gstatic.com/generate_204
  NullNode:
    type: http
    url: "https://nullnode.top/clashnode"
    path: ./nullnode.yaml
    interval: 3600
    health-check:
      enable: true
      interval: 600
      url: http://www.gstatic.com/generate_204
  OpenitNode:
    type: http
    url: "https://raw.fastgit.org/yu-steven/openit/main/Clash.yaml"
    path: OpenitNode.Clash/OpenitNode.yaml
    interval: 3600
    health-check:
      enable: true
      interval: 600
      url: http://www.gstatic.com/generate_204
